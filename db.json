			"name": "Pão Caseiro",
			"mealType": {
				"systemValue": 1,
				"displayValueEnglish": "breakfast",
				"displayValuePortuguese": "café da manhã"
			},
			"serverTo": 3,
			"difficultyLevel": {
				"systemValue": 1,
				"displayValueEnglish": "easy",
				"displayValuePortuguese": "fácil"
			},
			"ingredients": [
				{
					"id": 1,
					"description": "1 Kg de farinha de trigo",
					"alternativeIngredient": null
				},
				{
					"id": 2,
					"description": "1,5 xícara de leite morno",
					"alternativeIngredient": null
				},
				{
					"id": 3,
					"description": "0,5 xícara de água morna",
					"alternativeIngredient": null
				},
				{
					"id": 4,
					"description": "0,5 xícara de óleo",
					"alternativeIngredient": null
				},
				{
					"id": 5,
					"description": "4 colheres (sopa) de açucar",
					"alternativeIngredient": null
				},
				{
					"id": 6,
					"description": "1 colher (sopa) de sal",
					"alternativeIngredient": null
				},
				{
					"id": 7,
					"description": "2 ovos",
					"alternativeIngredient": null
				},
				{
					"id": 8,
					"description": "30g de fermento biológico fresco",
					"alternativeIngredient": {
						"id": 1,
						"description": "10g de fermento biológico seco",
						"alternativeIngredient": null
					}
				}
			],
			"preparationSteps": [
				{
					"id": 1,
					"description": "Em uma tigela, dissolva o fermento no açúcar e acrescente o sal, os ingredientes líquidos, os ovos e misture muito bem."
				},
				{
					"id": 2,
					"description": "Acrescente aos poucos a farinha até formar uma massa macia e sove bem a massa."
				},
				{
					"id": 3,
					"description": "Deixe a massa descansar por aproximadamente 1 hora."
				},
				{
					"id": 4,
					"description": "Após o crescimento, da a massa, enrole da forma que desejar, coloque nas formas e deixe crescer até dobrar de volume."
				},
				{
					"id": 5,
					"description": "Leve para assar em forno médio (200° C), preaquecido, por aproximadamente 30 minutos."
				},
				{
					"id": 6,
					"description": "Retire o pão do forno e pincele leite para a casca ficar mais macia."
				}
			],
			"urlImage": "https://img.itdg.com.br/tdg/images/recipes/000/178/357/328308/328308_original.jpg?mode=crop&width=710&height=400"
		},
